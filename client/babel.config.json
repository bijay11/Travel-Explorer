// this will add react transpilation process to webpack bundle process
{
  "presets": [
    // setup Babel TypeScript transpiling
    "@babel/preset-typescript",

    // Transpile and Polyfill to ES5
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-react"
  ]
}
